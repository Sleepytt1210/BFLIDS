{
    "name": "Hyperledger Fabric Gateway for BFLIDS",
    "version": "1.0.0",
    "description": "Blockchain-based Federated Learning implemented in typeScript using fabric-gateway",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "engines": {
        "node": ">=14",
        "npm": ">=5"
    },
    "scripts": {
        "build": "tsc",
        "build:watch": "tsc -w",
        "lint": "tslint -c tslint.json 'src/**/*.ts",
        "prepare": "npm run build",
        "pretest": "npm run lint",
        "start": "npm run build && node dist/src/app.js"
    },
    "engineStrict": true,
    "author": "Dylon Wong",
    "license": "Apache-2.0",
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/node": "^18.16.1",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "eslint": "^8.43.0",
        "typescript": "^5.1.6"
    },
    "dependencies": {
        "@hyperledger/fabric-gateway": "^1.3.0",
        "express": "^4.18.2",
        "http-status-codes": "^2.2.0"
    }
}
